ecoded_token\n    if auth_header\n      token = auth_header.split(&#39; &#39;)[1]\n      # header: { &#39;Authorization&#39;: &#39;Bearer &lt;token&gt;&#39; }\n      begin\n        JWT.decode(token, &#39;yourSecret&#39;, true, algorithm: &#39;HS256&#39;)\n      rescue JWT::DecodeError\n        nil\n      end\n    end\n  end\n\n  def logged_in_user\n    if decoded_token\n      user_id = decoded_token[0][&#39;user_id&#39;]\n      @user = User.find_by(id: user_id)\n    end\n  end\n\n  def logged_in?\n    !!logged_in_user\n  end\n\n  def authorized\n    render json: { message: &#39;Please log in&#39; }, status: :unauthorized unless logged_in?\n  end\nend</code></pre></div>","position":{"start":{"line":85,"column":1,"offset":1347},"end":{"line":125,"column":4,"offset":2192}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h1","properties":{},"children":[{"type":"text","value":"user controller","position":{"start":{"line":127,"column":3,"offset":2196},"end":{"line":127,"column":18,"offset":2211}}}],"position":{"start":{"line":127,"column":1,"offset":2194},"end":{"line":127,"column":18,"offset":2211}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"gatsby-highlight\" data-language=\"text\"><pre class=\"language-text\"><code class=\"language-text\">class UsersController &lt; ApplicationController\n  before_action :authorized, only: [:auto_login]\n\n  # REGISTER\n  def create\n    @user = User.create(user_params)\n    if @user.valid?\n      token = encode_token({user_id: @user.id})\n      render json: {user: @user, token: token}\n    else\n      render json: {error: &quot;Invalid username or password&quot;}\n    end\n  end\n\n  # LOGGING IN\n  def login\n    @user = User.find_by(username: params[:username])\n\n    if @user &amp;&amp; @user.authenticate(params[:password])\n      token = encode_token({user_id: @user.id})\n      render json: {user: @user, token: token}\n    else\n      render json: {error: &quot;Invalid username or password&quot;}\n    end\n  end\n\n\n  def auto_login\n    render json: @user\n  end\n\n  private\n\n  def user_params\n    params.permit(:username, :password, :age)\n  end\n\nend</code></pre></div>","position":{"start":{"line":129,"column":1,"offset":2213},"end":{"line":168,"column":4,"offset":3025}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h1","properties":{},"children":[{"type":"text","value":"guia test en POSTMAN","position":{"start":{"line":170,"column":3,"offset":3029},"end":{"line":170,"column":23,"offset":3049}}}],"position":{"start":{"line":170,"column":1,"offset":3027},"end":{"line":170,"column":23,"offset":3049}}},{"type":"text","value":"\n"},{"type":"element","tagName":"ul","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"REGISTER","position":{"start":{"line":172,"column":3,"offset":3053},"end":{"line":172,"column":11,"offset":3061}}}],"position":{"start":{"line":172,"column":1,"offset":3051},"end":{"line":172,"column":11,"offset":3061}}},{"type":"text","value":"\n"}],"position":{"start":{"line":172,"column":1,"offset":3051},"end":{"line":172,"column":11,"offset":3061}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"POST -> localhost:3000/register\nbody -> raw -> { \"username\":\"alfredo\", \"password\" : \"PassworsUPERS3CRET4\"}","position":{"start":{"line":174,"column":1,"offset":3063},"end":{"line":175,"column":75,"offset":3169}}}],"position":{"start":{"line":174,"column":1,"offset":3063},"end":{"line":175,"column":75,"offset":3169}}},{"type":"text","value":"\n"},{"type":"element","tagName":"ul","properties":{},"children":[{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"entrega token","position":{"start":{"line":177,"column":3,"offset":3173},"end":{"line":177,"column":16,"offset":3186}}}],"position":{"start":{"line":177,"column"